
public class Ex04_BookArray {
public static void main(String[] args) {
	int j = 5900;
	int h = 6900;
	int t = 7000;
	int bj = 5700;
	

	
	System.out.println("민선:" + j );
	System.out.println("효정:" + bj);
	System.out.println("정연:" + (bj + j + t));
	System.out.println("안나:" + (h + h));
	System.out.println("미연:" + (h + h));
	System.out.println("수현:" + (h + j+ j));
	
	
	
}
}
