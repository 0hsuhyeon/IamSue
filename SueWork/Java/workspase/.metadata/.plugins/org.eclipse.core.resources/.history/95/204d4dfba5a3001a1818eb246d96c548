import java.util.Scanner;

public class Ex02 {
	public static void main(String[] args) {
	
		Scanner s = new Scanner(System.in);
		
		int[][] arr = new int[3][4];
		
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr[i].length; j++) {
				System.out.printf("(%d, %d)\n",i,j);
				arr[i][j] = s.nextInt();
			}
		}
		
	}
}
